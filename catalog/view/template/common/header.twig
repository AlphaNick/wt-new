<body>
<div id="alert"></div>
<header class="sticky-h">
  <div class="homemenu">
  <div class="container">
    <div class="row">
      <div class="col-md-3 col-lg-2 col-sm-2 text-left">
        <div id="logo">
          {% if logo %}
            <a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-fluid"/></a>
          {% else %}
            <h1><a href="{{ home }}">{{ name }}</a></h1>
          {% endif %}
        </div>
      </div>
      <div class="col-lg-5 col-md-6 main-search">
      <div class="d-flex m-flex">
        <div class="top-search">
        {{ search }}    
        </div>
        <div class="top-contact-info t-none">
          <ul type="none">
              <li>{{ shop_day }}</li>
              <li>{{ shop_time }}</li>
          </ul>
        </div>
      </div>
      
      </div>
      <div id="top-links" class="col-md-3 col-sm-12 col-lg-5 text-end">
        <ul class="list-inline">
         <li>
           <div class="top-support t-none">
             <div>
               {{ text_fast_support }} 
             </div>
             <div>
               <span><b>{{ call_me }}</b></span>
             </div>
           </div>
         </li>
         <li class="userapp"></li>
         
         <li class="huser">
            <div class="dropdown">
              <a href="{{ account }}" class="dropdown-toggle huser-drop" data-bs-toggle="dropdown">
              <div class="xuser">                
                <img src="image/catalog/huser.svg" alt="user"/>
              </div>
              <div class="xuser-desc d-none">
                <span>{% if logged %}<span class="d-none d-lg-inline">{{ text_logout }}</span>{% else %}<span class="d-none d-lg-inline">{{ text_account }}</span>{% endif %}
              </span>
              </div>                
              </a>
              <ul class="dropdown-menu dropdown-menu-right">
                {% if not logged %}
                  <li><a href="{{ register }}" class="dropdown-item">{{ text_register }}</a></li>
                  <li><a href="{{ login }}" class="dropdown-item">{{ text_login }}</a></li>
                {% else %}
                  <li><a href="{{ account }}" class="dropdown-item">{{ text_account }}</a></li>
                  <li><a href="{{ order }}" class="dropdown-item">{{ text_order }}</a></li>
                  <li><a href="{{ transaction }}" class="dropdown-item">{{ text_transaction }}</a></li>
                  <li><a href="{{ download }}" class="dropdown-item">{{ text_download }}</a></li>
                  <li><a href="{{ logout }}" class="dropdown-item">{{ text_logout }}</a></li>
                {% endif %}
                <li class="xsla d-inline-block">{{ currency }}</li>
                <li class="xscu d-inline-block">{{ language }}</li>
              </ul>
              <ul>
                 
              </ul>
            </div>
        </li>
        <li class="list-inline-item top-wishlist"><a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}"><span class="d-md-inline wc-total">
          <img src="image/catalog/hwishlist.svg" alt="wishlist"> {{ text_wishlist }}</span></a></li>        
        <li id="header-cart">{{ cart }}</li>
      </ul>
      </div>
    </div>
  </div>
</div>
<div class="tablet-view d-block col-12 t-block cinfo">
   <div class="top-contact-info t-block d-flex justify-content-between align-items-center">
    <ul type="none">
      <li>{{ text_fast_support }} </li>
      <li>{{ call_me }}</li>
    </ul>
    <ul type="none">
      <li>{{ shop_day }}</li>
      <li>{{ shop_time }}</li>
    </ul>    
     <ul type="none">
    <li class="coll">{{ contact_mail }}</li>
    <li>{{ c_mail_address }}</li>
    </ul>
  </div>
</div>
<div class="menu-bg">
  <div class="container">
    <div class="row">
        <div class="col-md-3">
            {{ menu }}
        </div>      
        <div class="col-md-6 offer-line">
          <p><span>-30% off</span> for first order with 200 USD in the cart. <a href="#">Show more</a></p>
        </div>
        <div class="col-md-3 col-sm-3 text-end callright t-none">             
              <ul>
                <li>
                  <a href="index.php?route=information/information&language=en-gb&information_id=5">FAQ</a>
                  <a href="index.php?route=information/information&language=en-gb&information_id=1">About Us</a>
                  <a href="index.php?route=information/information&language=en-gb&information_id=3">Privacy Policy</a>
                </li>
              </ul>
        </div>
    </div>
  </div>  
</div>
</header>
<main>